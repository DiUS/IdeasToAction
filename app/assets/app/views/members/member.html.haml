#member.loading
  .header.main-header.follow-scroll(ng-click = '$parent.resetToTop()')
    %h1
      My profile
    .touch-panel
      %a.touch-icon(ng-href="/home")
        %i.icon-home
  .content.center
    .big {{interactions.length}}
    .big 
      %ng:pluralize(count="interactions.length" when="{'one': 'Action', 'other': 'Actions'}")
      Done
  .sections
    %collapsible(title='My actions' expanded="true" can-collapse="false" type='interaction' collection='interactions' item-template='assets/views/interactions/interactions_item.html')